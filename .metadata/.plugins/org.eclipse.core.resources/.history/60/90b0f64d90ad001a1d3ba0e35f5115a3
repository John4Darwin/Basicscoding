#include <stdio.h>
#include <stdlib.h>
int twoarray();
int main(void) {
int h;
setbuf(stdout,NULL);
h=twoarray();
	return EXIT_SUCCESS;
}
 int twoarray(){
	 int k,i,j,a[100],b[100],temp,L;
	printf("Enter a limit");
	scanf("%d",&L);
	printf("Enter values of array 1 ");
	for(i=0;i<L;i++){
		scanf("%d",&a[i]);
	}
	printf("Enter values of array 2");
	for(i=0;i<L;i++){
		scanf("%d",&b[i]);
	}
	for(i=0;i<L;i++){
		temp=a[i];
		a[i]=b[i];
		b[i]=temp;
	}
	for(i=0;i<L-1;i++){
		for(j=i+1;j<L;j++){
			if(a[i]>a[j]){
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}

		}
	}
	printf("Sorted swapped array 1 \n");
	for(i=0;i<L;i++){
		printf("%d\t\n",a[i]);
	}
	for(i=0;i<L-1;i++){
		for(j=0;j<L;j++){
			if(b[i]>b[j]);
			temp=b[i];
			b[i]=b[j];
			b[j]=temp;
		}
	}
	printf("Sorted swapped array 2 \n");
	for(i=0;i<L;i++){
		printf("%d\t",b[i]);
	}
	return k;
 }
