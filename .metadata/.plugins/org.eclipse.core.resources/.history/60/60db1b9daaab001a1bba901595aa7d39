#include <stdio.h>
#include <stdlib.h>
void array();
int main(void) {
arraysum();
	return EXIT_SUCCESS;
}
void arraysum(){
	int sum=0,i,L,v[100];
	setbuf(stdout,NULL);
	printf("Enter a limit");
	scanf("%d",&L);
	printf("Enter values");
	for(i=0;i<L;i++)
	{
		scanf("%d",&v[i]);
	}
	for(i=0;i<L;i++)
	{
		sum=sum+v[i];
	}


		printf("Sum is: %d",sum);
	}

}
