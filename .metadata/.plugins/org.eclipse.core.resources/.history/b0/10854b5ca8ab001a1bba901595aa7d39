#include <stdio.h>
#include <stdlib.h>
int array(int);
int main(void)
{
	int L,i,d;
	setbuf(stdout,NULL);
printf("Enter a limit");
scanf("%d",&L);
d=array(L);

printf("Entered values are \n");
	return EXIT_SUCCESS;
}
int array(int j)
{
	int i,v[100];
	printf("Enter values");
	for(i=0;i<j;i++)
	{
		scanf("%d",&v[j]);
	}

	for(i=0;i<j;i++)
	{
		printf("%d\t",v[i]);
	}
return i;
}
