#include <stdio.h>
#include <stdlib.h>
void grade();
int main(void) {
grade();
	return EXIT_SUCCESS;
}
void grade(){
	float bns,ca,gi,qt,cma,percentage;
	setbuf(stdout,NULL);
	printf("Enter the mark of 5 subject");
	scanf("%f%f%f%f%f",&bns,&ca,&gi,&qt,&cma);
	percentage=(bns+ca+gi+qt+cma)/100;
printf("Percentage is: %f",percentage);
}
