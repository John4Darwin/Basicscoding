#include <stdio.h>
#include <stdlib.h>
void array();
int main(void) {
array();
}
void array()
{
	int L,a[100],p;
	setbuf(stdout,NULL);
	printf("Enter array limit");
	scanf("%d",&L);
	printf("Enter values");
	for(p=0;p<L;p++)
	{
		scanf("%d",a[p]);
	}
	for(p=0;p<L;p++){
		printf("%d",a[p]);
	}
}
