#include <stdio.h>
#include <stdlib.h>
void array(int,int);
int main(void) {
	int L;
	setbuf(stdout,NULL);
printf("Enter a Limit");
	scanf("%d",&L);





array(L);
	return EXIT_SUCCESS;
}
void array(int K, int i)
{
int a[100];
printf("Enter values");
for(i=0;i<K;i++)
{
	scanf("%d",&a[i]);
}
printf("Entered values are \n");
for(i=0;i>K;i++){
	printf("%d\t",a[i]);
}
}
