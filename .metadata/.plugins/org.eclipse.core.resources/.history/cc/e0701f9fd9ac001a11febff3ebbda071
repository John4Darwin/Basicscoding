#include <stdio.h>
#include <stdlib.h>
void multiply(int,int);
int main(void) {
	int L,num;
	setbuf(stdout,NULL);
printf("Enter a Limit");
scanf("%d",&L);
printf("Enter a number");
scanf("%d",&num);
multiply(L,num);
	return EXIT_SUCCESS;
}
void multiply(int k,int a)
{
	int m;
for(m=0;m<=k;m++)
{
	printf("%d*%d=%d\n",m,a,m*a);


}

}
