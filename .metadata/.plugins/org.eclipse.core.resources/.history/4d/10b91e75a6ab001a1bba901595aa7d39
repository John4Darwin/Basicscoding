#include <stdio.h>
#include <stdlib.h>
int array(int);
int main(void)
{
	int L,d,a[100];
	setbuf(stdout,NULL);
printf("Enter a limit");
scanf("%d",&L);
d=array(L);
printf("Entered values are :\n");
for(d=0;d<L;d++){
	printf("%d\t",a[d]);
}
	return EXIT_SUCCESS;
}
int array(int j)
{
	int i,c,v[100];
	printf("Enter values");
	for(i=0;i<j;i++)
	{
		scanf("%d",&v[j]);
	}
return c;
}
