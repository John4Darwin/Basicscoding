#include <stdio.h>
#include <stdlib.h>
void multiplication();
int main(void) {
	multiplication();
	return EXIT_SUCCESS;
}
void multiplication(){
	int num,i;
	setbuf(stdout,NULL);
	printf("Enter a number");
	scanf("%d",&num);
	for(i=1;i<num;i++){
			printf("%d*%d=%d",num,i,i*num);
		}
	}

