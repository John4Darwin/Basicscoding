#include <stdio.h>
#include <stdlib.h>
void multiplication();
int main(void) {

	multiplication();
	return EXIT_SUCCESS;
}
void multiplication(){
	int num,i,L=10;
	setbuf(stdout,NULL);
	printf("Enter a number");
	scanf("%d",&num);
	for(i=1;i<L;i++){
			printf("%d*%d=%d\n",num,i,num*i);
		}
	}

