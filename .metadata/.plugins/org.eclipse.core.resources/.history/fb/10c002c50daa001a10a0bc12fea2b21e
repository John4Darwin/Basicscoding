#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int V[100],i,L,Ev;
	setbuf(stdout,NULL);
printf("Enter Array Limit");
scanf("%d",&L);
printf("Enter values ");
for(i=0;i<L;i++){
	scanf("%d",&V[i]);
}
for(i=0;i<L;i++){
if(V[i]%2==0){
scanf("%d",&Ev);
}
}
for(i=0;i<L;i++){
	printf("%d",Ev);
}
return EXIT_SUCCESS;
}
