#include <stdio.h>
#include <stdlib.h>

int main(void) {
int	V[3][3],j,i;
setbuf(stdout,NULL);
printf("Enter  values");
for(i=0;i<3;i++){
	for(j=0;j<3;j++){
	scanf("%d",&V[i][j]);
	}
}
printf("Entered values are \n");
for(i=0;i<3;i++){
	for(j=0;j<i;j++){
		printf("%d\n",V[i][j]);
	}
}
	return EXIT_SUCCESS;
}
