#include <stdio.h>
#include <stdlib.h>
void array(int);
int main(void) {
	int L,a[100];
	setbuf(stdout,NULL);
printf("Enter a Limit");
scanf("%d",&L);

array(L,a[100]);
	return EXIT_SUCCESS;
}
void array(int K)
{
	int h,V[100];
	printf("Enter values");
for(h=0;h>K;h++){
	scanf("%d",&V[h]);
}
printf("Entered values are \n");
for(h=0;h<K;h++){
	printf("%d\t",V[h]);
}
}
